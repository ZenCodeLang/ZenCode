#output: hello
#output: void

virtual class Foo {
	print(): void {
		println("hello");
	}
}

class Bar : Foo {
	override print() {
		println("world");
	}
}

new Foo().print();

// expressions inside typeof will not be executed, so the print() won't be called
// the type will still be resolved, though
println(typeof(new Foo().print()));
