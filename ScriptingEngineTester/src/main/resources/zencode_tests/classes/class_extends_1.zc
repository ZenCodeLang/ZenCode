#output: hello thing
#output: world
#output: goodbye thing

virtual class Foo {
  val name as string;

  sayHello(): void {
	println("hello " + this.name);
  }

  sayGoodbye(): void {
  	println("goodbye " + this.name);
  }
}

class Bar : Foo {
  sayHello(): void {
	super.sayHello();
	println("world");
  }
}

new Bar("thing").sayHello();
new Bar("thing").sayGoodbye();
