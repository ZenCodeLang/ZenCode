// One of the tests required to make StdLib (Arrays.zs) work
public expand <T> T[] {
	public reversed() as T[] {
       var result = new T[](this.length);
       for i in 0 .. length {
           result[i] = this[length - i];
       }

   	return result;
   }
}


var arr = ["Hello", "World"];
var rev = arr.reversed();
for it in rev {
    println(it);
}