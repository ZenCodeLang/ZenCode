#output: 1
#output: (1|1)
#output: end 1
#output: 2
#output: end all

val nums = [1, 2, 3, 4];

for x in nums {
    println(x);
    if (x % 2 == 0) break;
    for y in nums {
        if (y % 2 == 0) break;
        println("(" + x + "|" + y + ")");
    }
    println("end " + x);
}
println("end all");
